le: {\r\n      type: String,\r\n      default: \"Laboratorio\",\r\n    },\r\n    backgroundImage: {\r\n      default: \"img/sidebar-5.jpg\",\r\n    },\r\n  },\r\n  computed: {\r\n    sidebarStyle() {\r\n      return {\r\n        backgroundImage: `url(${this.backgroundImage})`,\r\n      };\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.sidebar {\r\n  width: 250px;\r\n  height: 100vh; /* Ocupar todo el alto de la p√°gina */\r\n  background-size: cover;\r\n  background-position: center center;\r\n  background-color: rgba(0, 0, 0, 0.8);\r\n}\r\n\r\n.sidebar .sidebar-wrapper {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.sidebar .logo .logo__container {\r\n  display: flex;\r\n  align-items: center;\r\n  padding-left: 10px;\r\n  border-bottom: 1px solid #ffffff;\r\n}\r\n\r\n.sidebar .logo-img img {\r\n  width: 40px;\r\n  height: 40px;\r\n}\r\n\r\n.sidebar .menu a {\r\n  display: flex;\r\n  align-items: center;\r\n  color: #ffffff;\r\n  text-decoration: none;\r\n  transition: color 0.3s;\r\n  padding: 10px 20px;\r\n  border-radius: 5px;\r\n  margin-bottom: 5px;\r\n}\r\n\r\n.sidebar .menu i {\r\n  margin-left: 10px;\r\n}\r\n</style>\r\n"],"mappings":"AAsCA,OAAOA,WAAU,MAAO,YAAY;AAEpC,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAI,EAAE,YAAY;EAClBC,KAAK,EAAE;IACLC,KAAK,EAAE;MACLC,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE;IACX,CAAC;IACDC,eAAe,EAAE;MACfD,OAAO,EAAE;IACX;EACF,CAAC;EACDE,QAAQ,EAAE;IACRC,YAAYA,CAAA,EAAG;MACb,OAAO;QACLF,eAAe,EAAG,OAAM,IAAI,CAACA,eAAgB;MAC/C,CAAC;IACH;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}