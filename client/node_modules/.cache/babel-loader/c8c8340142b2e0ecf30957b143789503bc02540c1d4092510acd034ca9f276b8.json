{"ast":null,"code":"import SidebarLink from \"./SidebarLink.vue\";\nexport default {\n  components: {\n    SidebarLink\n  },\n  props: {\n    title: {\n      type: String,\n      default: \"Vue LBD\"\n    },\n    backgroundColor: {\n      type: String,\n      default: \"black\",\n      validator: value => {\n        let acceptedValues = [\"\", \"blue\", \"azure\", \"green\", \"orange\", \"red\", \"purple\", \"black\"];\n        return acceptedValues.indexOf(value) !== -1;\n      }\n    },\n    backgroundImage: {\n      type: String,\n      default: \"img/sidebar-5.jpg\"\n    },\n    activeColor: {\n      type: String,\n      default: \"success\",\n      validator: value => {\n        let acceptedValues = [\"primary\", \"info\", \"success\", \"warning\", \"danger\"];\n        return acceptedValues.indexOf(value) !== -1;\n      }\n    },\n    sidebarLinks: {\n      type: Array,\n      default: () => []\n    },\n    autoClose: {\n      type: Boolean,\n      default: true\n    }\n  },\n  provide() {\n    return {\n      autoClose: this.autoClose\n    };\n  },\n  computed: {\n    sidebarStyle() {\n      return {\n        backgroundImage: `url(${this.backgroundImage})`\n      };\n    }\n  }\n};","map":{"version":3,"names":["SidebarLink","components","props","title","type","String","default","backgroundColor","validator","value","acceptedValues","indexOf","backgroundImage","activeColor","sidebarLinks","Array","autoClose","Boolean","provide","computed","sidebarStyle"],"sources":["D:\\WEB\\Servicios\\ProyectoFinal\\client\\src\\components\\sidebar\\sideBar.vue"],"sourcesContent":["<template>\r\n  <div\r\n    class=\"sidebar\"\r\n    :style=\"sidebarStyle\"\r\n    :data-color=\"backgroundColor\"\r\n    :data-image=\"backgroundImage\"\r\n  >\r\n    <div class=\"sidebar-wrapper\">\r\n      <div class=\"logo\">\r\n        <a href=\"#\" class=\"simple-text logo__container\">\r\n          <div class=\"logo-img\">\r\n            <img src=\"img/vue-logo.png\" alt=\"\" />\r\n          </div>\r\n          {{ title }}\r\n        </a>\r\n      </div>\r\n\r\n      <slot name=\"content\"></slot>\r\n      <ul class=\"nav nav-main__links\">\r\n        <!--By default vue-router adds an active class to each route link. This way the links are colored when clicked-->\r\n        <slot>\r\n          <sidebar-link\r\n            v-for=\"(link, index) in sidebarLinks\"\r\n            :key=\"link.name + index\"\r\n            :to=\"link.path\"\r\n            @click=\"closeNavbar\"\r\n            :link=\"link\"\r\n          >\r\n            <i :class=\"link.icon\"></i>\r\n            <p>{{ link.name }}</p>\r\n          </sidebar-link>\r\n        </slot>\r\n      </ul>\r\n      <ul class=\"nav nav-bottom\" v-if=\"$slots['bottom-links']\">\r\n        <slot name=\"bottom-links\"></slot>\r\n      </ul>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport SidebarLink from \"./SidebarLink.vue\";\r\n\r\nexport default {\r\n  components: {\r\n    SidebarLink,\r\n  },\r\n  props: {\r\n    title: {\r\n      type: String,\r\n      default: \"Vue LBD\",\r\n    },\r\n    backgroundColor: {\r\n      type: String,\r\n      default: \"black\",\r\n      validator: (value) => {\r\n        let acceptedValues = [\r\n          \"\",\r\n          \"blue\",\r\n          \"azure\",\r\n          \"green\",\r\n          \"orange\",\r\n          \"red\",\r\n          \"purple\",\r\n          \"black\",\r\n        ];\r\n        return acceptedValues.indexOf(value) !== -1;\r\n      },\r\n    },\r\n    backgroundImage: {\r\n      type: String,\r\n      default: \"img/sidebar-5.jpg\",\r\n    },\r\n    activeColor: {\r\n      type: String,\r\n      default: \"success\",\r\n      validator: (value) => {\r\n        let acceptedValues = [\r\n          \"primary\",\r\n          \"info\",\r\n          \"success\",\r\n          \"warning\",\r\n          \"danger\",\r\n        ];\r\n        return acceptedValues.indexOf(value) !== -1;\r\n      },\r\n    },\r\n    sidebarLinks: {\r\n      type: Array,\r\n      default: () => [],\r\n    },\r\n    autoClose: {\r\n      type: Boolean,\r\n      default: true,\r\n    },\r\n  },\r\n  provide() {\r\n    return {\r\n      autoClose: this.autoClose,\r\n    };\r\n  },\r\n  computed: {\r\n    sidebarStyle() {\r\n      return {\r\n        backgroundImage: `url(${this.backgroundImage})`,\r\n      };\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style>\r\n.sidebar .sidebar-wrapper {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n.sidebar .nav-main__links {\r\n  flex: 1;\r\n}\r\n.sidebar .sidebar-wrapper .logo .logo__container {\r\n  padding-left: 10px;\r\n}\r\n</style>\r\n"],"mappings":"AAwCA,OAAOA,WAAU,MAAO,mBAAmB;AAE3C,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,KAAK,EAAE;IACLC,KAAK,EAAE;MACLC,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE;IACX,CAAC;IACDC,eAAe,EAAE;MACfH,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE,OAAO;MAChBE,SAAS,EAAGC,KAAK,IAAK;QACpB,IAAIC,cAAa,GAAI,CACnB,EAAE,EACF,MAAM,EACN,OAAO,EACP,OAAO,EACP,QAAQ,EACR,KAAK,EACL,QAAQ,EACR,OAAO,CACR;QACD,OAAOA,cAAc,CAACC,OAAO,CAACF,KAAK,MAAM,CAAC,CAAC;MAC7C;IACF,CAAC;IACDG,eAAe,EAAE;MACfR,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE;IACX,CAAC;IACDO,WAAW,EAAE;MACXT,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE,SAAS;MAClBE,SAAS,EAAGC,KAAK,IAAK;QACpB,IAAIC,cAAa,GAAI,CACnB,SAAS,EACT,MAAM,EACN,SAAS,EACT,SAAS,EACT,QAAQ,CACT;QACD,OAAOA,cAAc,CAACC,OAAO,CAACF,KAAK,MAAM,CAAC,CAAC;MAC7C;IACF,CAAC;IACDK,YAAY,EAAE;MACZV,IAAI,EAAEW,KAAK;MACXT,OAAO,EAAEA,CAAA,KAAM;IACjB,CAAC;IACDU,SAAS,EAAE;MACTZ,IAAI,EAAEa,OAAO;MACbX,OAAO,EAAE;IACX;EACF,CAAC;EACDY,OAAOA,CAAA,EAAG;IACR,OAAO;MACLF,SAAS,EAAE,IAAI,CAACA;IAClB,CAAC;EACH,CAAC;EACDG,QAAQ,EAAE;IACRC,YAAYA,CAAA,EAAG;MACb,OAAO;QACLR,eAAe,EAAG,OAAM,IAAI,CAACA,eAAgB;MAC/C,CAAC;IACH;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}